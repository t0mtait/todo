<div class="completed">
    <h2 class="mb-2 text-lg font-semibold text-gray-900 dark:text-white">Completed tasks:</h2>
            <% if (locals.completed.length > 0) { %>
                <ul class="max-w-md space-y-1 text-gray-900 list-disc list-none list-inside dark:text-gray-100">
                    <% completed.forEach(function(completed) { %>
                        <li style="display: flex; align-items: center;">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" style="fill: rgb(4, 172, 4);transform: ;msFilter:;"><path d="M19 3H5c-1.103 0-2 .897-2 2v14c0 1.103.897 2 2 2h14c1.103 0 2-.897 2-2V5c0-1.103-.897-2-2-2zm-7.933 13.481-3.774-3.774 1.414-1.414 2.226 2.226 4.299-5.159 1.537 1.28-5.702 6.841z"></path></svg>
                            <%= completed %>
                        </li>
    
                    <% }) %>
                </ul>
            <% } else { %>
                <p>
                    no todos!
                </p>
            <% } %>
</div>